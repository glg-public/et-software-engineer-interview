{
  "name": "et-software-engineer-interview",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "lint": "npm run prettier:check && npm run eslint:check && npm run typecheck",
    "eslint:check": "eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noemit",
    "tscompile:docker": "tsc --build tsconfig.prod.json && tsc-alias -p tsconfig.prod.json",
    "tscompile": "tsc --build tsconfig.json && tsc-alias -p tsconfig.json",
    "start:prod": "node dist/server.js",
    "start:dev": "nodemon --exec 'ts-node --esm' src/server.ts",
    "start:debug": "nodemon --exec \"node --loader ts-node/esm --inspect=0.0.0.0:${DEBUG_PORT:-9229}\" src/server.ts",
    "test": "npm run lint && npm run test:jest",
    "test:jest": "jest",
    "test:clean": "npm run test:jest -- --clearCache",
    "prettier:check": "npm run prettier:fix && prettier --config .prettierrc.json 'src/**/*.ts' --check",
    "prettier:fix": "prettier --config .prettierrc.json 'src/**/*.ts' --write",
    "fix": "npm run eslint:fix && npm run prettier:fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/router": "12.0.1",
    "http-status-codes": "2.3.0",
    "koa": "2.15.0",
    "koa-bodyparser": "4.4.1",
    "koa-helmet": "7.0.2",
    "mssql": "10.0.2",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@types/jest": "~29.5.5",
    "@types/koa": "~2.13.12",
    "@types/koa__router": "~12.0.4",
    "@types/koa-bodyparser": "~4.3.12",
    "@types/mssql": "^9.1.5",
    "@types/node": "~18.16.19",
    "@typescript-eslint/eslint-plugin": "~6.7.2",
    "@typescript-eslint/parser": "~6.7.2",
    "eslint": "~8.49.0",
    "eslint-config-prettier": "~9.0.0",
    "eslint-import-resolver-typescript": "~3.6.0",
    "eslint-plugin-import": "~2.28.1",
    "eslint-plugin-jest-formatting": "~3.1.0",
    "eslint-plugin-unicorn": "~48.0.1",
    "jest": "~29.7.0",
    "nodemon": "~3.0.1",
    "prettier": "~3.0.3",
    "ts-jest": "~29.1.1",
    "ts-node": "~10.9.1",
    "tsc-alias": "~1.8.8",
    "tsconfig-paths": "~4.2.0",
    "tsutils": "~3.21.0"
  },
  "engines": {
    "node": ">= 18 <19"
  }
}
